{
    "manifest_version": 3,
    "name": "Budget Patrol",
    "version": "1.0",
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],   
        "js": ["content.bundle.js"]
      }
    ],
    "web_accessible_resources": [
      {
        "resources": ["content.css"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  